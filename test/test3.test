class Rectangle {

    private int width, height;

    static protected char* sample = "a test !";

    public int set_width(const int new_width) {
        width = new_width;
        return width;
    }

    public int get_width() {
        return width;
    }

    public int set_height(const int new_height) {
        height = new_height;
        return height;
    }

    public int get_height() {
        return height;
    }

    public int get_area() {
        return width * height;
    }

    public string get_sample_with_hello_msg(string hello_msg) {
        return to_string(sample) + hello_msg;
    }

    public int do_while_test() {

        int i = 0;

        do {
            putc(sample[i]);
            i++;
        } while (i < strlen(teste));

        return 0;
    }

    public int while_test() {

        int i = 0;

        while(i < strlen(teste)) {
            putc(sample[i]);
            i++;
        }

        return 0;
    }

    public int for_test() {

        for(int i = 0; i < strlen(teste); i++) {
            putc(sample[i]);
            i++;
        }

        return 0;
    }

    public int do_while_test_with_if() {

        int i = 0;

        do {
            if(i%2 == 0)
                putc(sample[i]);
            i++;
        } while (i < strlen(teste));

        return 0;
    }

    public int while_test_with_if() {

        int i = 0;

        while(i < strlen(teste)) {
            if(i <= 5) {
                putc(sample[i]);
            } else {
                putc(toupper(sample[i]))
            } 
            i++;
        }

        return 0;
    }

    public int for_test_with_if() {

        for(int i = 0; i < strlen(teste); i++) {
            if(i%3 == 0)
                continue;
            if(i%11 == 0)
                break;
            putc(sample[i]);
            i++;
        }

        return 0;
    }
}